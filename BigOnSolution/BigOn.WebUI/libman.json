{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/libs/toastr.js/"
    },
    {
      "library": "sweetalert@2.1.2",
      "destination": "wwwroot/libs/sweetalert/"
    },
    {
      "library": "ckeditor5@35.1.0",
      "destination": "wwwroot/libs/ckeditor5/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/libs/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}